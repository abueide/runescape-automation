<manifest>
    <main-class>arch.bots.archzulrahtrainer.ArchZulrahTrainer</main-class>
    <name>Arch Zulrah Trainer</name>
    <tag-line>Are you tired of getting screwed by a snake?</tag-line><!--Max of 50 chars-->
    <description>The best zulrah trainer</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>Other</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>nullifer.archzulrahtrainer</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>other</tag>
    </tags>
    <resources>
        <resource>src/resources/arch/bots/archzulrahtrainer/ui/fxml/main.fxml</resource>
        <resource>src/resources/arch/bots/archzulrahtrainer/ui/fxml/skillpane.fxml</resource>
        <resource>src/resources/arch/bots/archzulrahtrainer/ui/fxml/background.css</resource>
        <resource>src/resources/arch/bots/archzulrahtrainer/ui/images/space.png</resource>
    </resources>
</manifest>