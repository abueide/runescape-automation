<manifest>
    <main-class>archbots.nullmage.NullMage</main-class>
    <name>Null Mage</name>
    <tag-line>Shitty mage training</tag-line><!--Max of 50 chars-->
    <description>High Alchs rune arrows and casts stun on the nearest entity named "Grizzly bear".
    </description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>Magic</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>nullifer.nullmage</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>magic</tag>
    </tags>
    <resources>
        <resource>com/archmage/nullmage/main.fxml</resource>
    </resources>
</manifest>