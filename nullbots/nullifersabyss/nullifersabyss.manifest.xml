<manifest>
    <main-class>nullbots.nullifersabyss.NullifersAbyss</main-class>
    <name>Nullifer's Abyss</name>
    <tag-line>Once you enter my abyss you will never leave...</tag-line><!--Max of 50 chars-->
    <description>Crafts runes in the abyss with a great feature set.</description><!--Max of 110 chars-->
    <version>1.0.2</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>RUNECRAFTING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>nullifer.nullifersabyss</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>abyss</tag>
        <tag>runecrafting</tag>
        <tag>nature</tag>
        <tag>runes</tag>
        <tag>law</tag>
        <tag>cosmic</tag>
        <tag>aio</tag>
    </tags>
    <resources>
        <resource>com/nullbots/nullifersabyss/ui/view/main.fxml</resource>
    </resources>
</manifest>