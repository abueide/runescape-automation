<manifest>
    <main-class>archbots.kotlintemplate.KotlinTemplate</main-class>
    <name>Kotlin Bot Template</name>
    <tag-line>Shitty mage training</tag-line><!--Max of 50 chars-->
    <description>A great template</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>Magic</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>nullifer.kotlintemplate</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>other</tag>
    </tags>
    <resources>
        <resource>src/resources/archbots/kotlintemplate/ui/fxml/main.fxml</resource>
        <resource>src/resources/archbots/kotlintemplate/ui/fxml/skillpane.fxml</resource>
        <resource>src/resources/archbots/kotlintemplate/ui/fxml/background.css</resource>
        <resource>src/resources/archbots/kotlintemplate/ui/images/space.png</resource>
    </resources>
</manifest>