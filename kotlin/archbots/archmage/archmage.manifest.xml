<manifest>
    <main-class>archbots.archmage.ArchMage</main-class>
    <name>Arch Mage</name>
    <tag-line>Become a crimson arch mage</tag-line><!--Max of 50 chars-->
    <description>The best AIO magic trainer</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>Magic</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>nullifer.archmage</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>other</tag>
    </tags>
    <resources>
        <resource>archmage/archmage/ui/view/main.fxml</resource>
    </resources>
</manifest>